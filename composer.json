{
  "name": "joonlabs/php-graphql",
  "type": "library",
  "description": "A PHP implementation of the GraphQL specifications",
  "homepage": "https://github.com/joonlabs/php-graphql",
  "license": "MIT",
  "authors": [
    {
      "name": "joon",
      "email": "info@joonlabs.com"
    }
  ],
  "keywords": [
    "graphql",
    "API"
  ],
  "require-dev": {
    "phpunit/phpunit": "9.5.4",
    "phpbench/phpbench": "1.0.1"
  },
  "require": {
    "php": ">=7.3"
  },
  "autoload": {
    "psr-4": {
      "GraphQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GraphQL\\Tests\\": "tests/",
      "GraphQL\\Benchmarks\\": "tests/"
    }
  },
  "scripts": {
    "bench": "phpbench run",
    "test": "phpunit tests"
  }
}